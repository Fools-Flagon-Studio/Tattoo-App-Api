  "main": "index.js",
  "scripts": {
    "docker-test:up": "docker compose -f docker/docker-test-compose.yml up -d",
    "docker-test:down": "docker compose -f docker/docker-test-compose.yml down --volumes",
    "test": "cross-env NODE_ENV=test npm run docker-test:up && cross-env NODE_ENV=test npm run wait && cross-env NODE_ENV=test jest --forceExit",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down --volumes",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "wait": "node utils/waitForDb.js",
    "launch": "npm run docker:up && npm run wait && npm run migrate && npm run dev"
  },
